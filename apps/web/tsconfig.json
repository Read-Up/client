{
  "extends": "@readup/typescript-config/nextjs.json",
  "compilerOptions": {
    "typeRoots": [
      "./app/_types/common/*",
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}